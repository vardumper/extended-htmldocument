{# teaser-example-two #}
{% block teaser-example-two %}
    {% apply spaceless }
    <div class="teaser">
        <hgroup>
            <h3>{{ entry.title }}</h3>
            <h4>{{ entry.subtitle }}</h4>
        </hgroup>
        <p>{{ entry.description }}</p>
        <img alt="{{ entry.image.alt }}" src="{{ entry.image.url }}">
        <a href="{{ entry.url }}" role="button">{{ 'Read more'|t }}</a>
    </div>
    {% endapply %}
{% endblock %}
